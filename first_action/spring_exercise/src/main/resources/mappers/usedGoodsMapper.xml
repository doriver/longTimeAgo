<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.exercise.ex02.dao.UsedGoodsDAO">
 	<select id="selectUsedGoodsList" resultType="com.exercise.ex02.model.UsedGoods">
 		SELECT
 			`id`
 			, `title`
 			, `price`
 			, `description`
 			, `picture`
 			, `sellerId`
 			, `createdAt`
 			, `updatedAt`
 		FROM
 			`used_goods`	
 	</select>
 
 </mapper>